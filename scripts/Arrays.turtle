func push(x) {
    return this + [x];
}

Array.push = push;

Array.forEach = (f) => {
    for let i = 0; i < this.length; i = i + 1 {
        f(this[i], i);
    }
};

Array.find = (f) => {
    for let i = 0; i < this.length; i = i + 1 {
        if (f(this[i], i)) {
            return this[i];
        }
    }
    return nil;
};
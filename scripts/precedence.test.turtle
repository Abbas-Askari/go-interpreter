// === Equality tests ===
let a = 1;
let b = 2;
let c = 1;

print a == b;    // false
print a == c;    // true
print a != b;    // true
print a != c;    // false

// === Comparison tests ===
print a < b;     // true
print b < a;     // false
print a <= b;    // true
print b <= a;    // false
print a <= c;    // true

print b > a;     // true
print a > b;     // false
print b >= a;    // true
print a >= b;    // false
print a >= c;    // true

// === Logical AND (&&) ===
print true && true;   // true
print true && false;  // false
print false && true;  // false
print false && false; // false

// === Logical OR (||) ===
print true || true;   // true
print true || false;  // true
print false || true;  // true
print false || false; // false

// === Mixed precedence ===
// Equality vs. comparison
print (a == c) && (b > a);  // true && true => true
print (a == b) || (b > a);  // false || true => true
print (a != b) && (a < b);  // true && true => true
print (a != b) || (a > b);  // true || false => true

// === Precedence without parens ===
// Comparison binds tighter than &&
print a < b && b < 3;  // (a < b) && (b < 3) => true
print a == c || b < a; // (a == c) || (b < a) => true

// Parens should override precedence
print a == (b < c);    // (b < c) => false, a == false => false
print (a == b) || (c == a); // false || true => true

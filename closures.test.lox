let x = "global";
func outer() {
  let x = "outer";
  func inner() {
    print x;
    x = x + " changed from inner";
    print x;
  }

  func next() {
    print x;
    x = x + 'changed from next';
    print x;
  }


  inner();
  print x;
  next();
  inner();
  print x;
  return x;
}
print "Func returns " + outer();